{
  "name": "uct-css-tip",
  "private": true,
  "displayName": "uct-css-tip",
  "description": "uct-css-tip",
  "keywords": [
    "vscode",
    "plugin",
    "uct",
    "css"
  ],
  "version": "0.0.1",
  "publisher": "uct",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "uct-css-tip",
      "properties": {
        "uctCssTip.file": {
          "type": "string",
          "default": "",
          "description": "需要提示的加载css路径（全路径）"
        }
      }
    }
  },
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crazyfdf/uct-theme/issues"
  },
  "repository": {
    "type": "git",
